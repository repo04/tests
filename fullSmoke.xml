<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="FullSmoke Suite">
    <listeners>
        <listener class-name="runThrghTestNG.TransformSmoke"/>
        <listener class-name="runThrghTestNG.TestInvocation"/>
    </listeners>
    <test name="SmokeTests" preserve-order="true" parallel="false">
        <groups>
            <run>
                <include name="prerequisite"/>
                <include name="fullSmoke"/>
            </run>
        </groups>
        <classes>
            <class name="runThrghTestNG.ContentAdmin_Course_GroupCourseCreation"/>
            <class name="runThrghTestNG.Pes_UserCreation_AssignRole_WorkingGroup"/>
            <class name="runThrghTestNG.Teacher_Posts_SocialGroup"/>
            <class name="runThrghTestNG.Student_JoinSocialGroup_Post"/>
            <class name="runThrghTestNG.Teacher_LiveSession_GoogleDoc"/>
            <class name="runThrghTestNG.Teacher_FetchActivityPassword"/>
            <class name="runThrghTestNG.Student_LiveSession_SocialGroup_GoogleDoc"/>            
            <class name="runThrghTestNG.Teacher_JoinDelete_SocialGroup"/>
            <class name="runThrghTestNG.Student_DeleteSocialGroup"/>
            <class name="runThrghTestNG.Teacher_ResubmitAllInOne"/>            
            <class name="runThrghTestNG.Student_AllInOneEnable"/>
            <class name="runThrghTestNG.ContentAdmin_CleanTestData"/>
            <class name="runThrghTestNG.Pes_CleanTestData"/>
            <class name="runThrghTestNG.Pes_ArchiveCourse"/>
            <!--class name="runThrghTestNG.Teacher_VerifyEmailNotifications"/>
            <class name="runThrghTestNG.Student_VerifyEmailNotifications"/-->
        </classes>
    </test>
</suite>
        <parameter name="studentSocialGroupName" value="AutoStdtSclGrp"/>         
         
        <groups>
            <define name="Settings">
                <include name="prerequisite"/>
                <include name="settings.teacherVerify"/>
                <include name="settings.studentVerify"/>                                
            </define>            
        </groups>
        <classes>

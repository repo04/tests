<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="Staging-DebugSuite" verbose="5" parallel="tests" thread-count="10">
    <listeners>
        <listener class-name="com.lms.tests.runThrghTestNG.TestInvocation"/>
    </listeners>
    <test name="DebugTests_1" preserve-order="true" parallel="false">
        <parameter name="url" value="https://2gu.nursing.georgetown.edu"/>
        <parameter name="program" value="gu-msn"/>
        <parameter name="browser" value="ff"/>
        <parameter name="os" value="win"/>

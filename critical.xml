                <include name="prerequisite"/>
                <include name="criticalsmoke"/>
            </run>
        </groups>
        <classes>
            <class name="runThrghTestNG.CntAdmin_Crs_GrpCrsCreation"/>
            <class name="runThrghTestNG.Pes_UsrCrtn_AsgnRole_WrkngGrp"/>
            <class name="runThrghTestNG.Tchr_Posts_SclGrp"/>
            <class name="runThrghTestNG.Stdt_JnSclGrp_Post"/>
            <class name="runThrghTestNG.Tchr_LvSsn_GglDoc"/>
            <class name="runThrghTestNG.Stdt_LvSsn_SclGrp_GglDoc"/>            
            <class name="runThrghTestNG.Tchr_JoinDelete_SclGrp"/>
            <class name="runThrghTestNG.Stdt_DeleteSclGrp"/>
            <class name="runThrghTestNG.Pes_CleanTestData"/>
            <!--class name="runThrghTestNG.Tchr_VerifyEmailNotifications"/>
            <class name="runThrghTestNG.Stdt_VerifyEmailNotifications"/-->                                              
        </classes>

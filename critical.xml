<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="CriticalSmoke Suite">
    <listeners>
        <listener class-name="runThrghTestNG.TransformCritical"/>
        <listener class-name="runThrghTestNG.TestInvocation"/>
    </listeners>
    <test name="CriticalTests" preserve-order="true" parallel="false">
        <parameter name="groupCourseName" value="AutoGroupCourse"/>
        <parameter name="quizActivityName" value="AutoQuiz"/>
        <parameter name="passwordQuizName" value="AutoPswdQuiz"/>
        <groups>
            <run>
                <include name="prerequisite"/>
                <include name="criticalSmoke"/>
            </run>
        </groups>
        <classes>
            <class name="runThrghTestNG.Teacher_Posts_SocialGroup"/>
            <class name="runThrghTestNG.Student_JoinSocialGroup_Post"/>
            <class name="runThrghTestNG.Teacher_LiveSession_GoogleDoc"/>
            <class name="runThrghTestNG.Student_LiveSession_SocialGroup_GoogleDoc"/>            
            <class name="runThrghTestNG.Teacher_JoinDelete_SocialGroup"/>
            <class name="runThrghTestNG.Student_DeleteSocialGroup"/>
        </classes>
    </test>    
</suite>